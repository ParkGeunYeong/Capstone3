{
    "name" : "hello extensions",
    "description" : "test1",
    "version" : "1.0",
    "manifest_version" : 2,
    "permissions": [
      "activeTab"
    ],
    "browser_action" : {
        "default_popup" : "hello.html",
        "default_icon" : "hello_extensions.png"
    },
    "content_scripts": [
      {
        "matches": ["*://*/*"],
        "run_at": "document_idle",
        "js": ["contentScript.js"]
      }
    ],
    "commands": {
        "_execute_browser_action": {
          "suggested_key": {
            "default": "Ctrl+Shift+F",
            "mac": "MacCtrl+Shift+F"
          },
          "description": "Opens hello.html"
        }
      }
}